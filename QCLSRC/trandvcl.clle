             ENDDO
             IF         ((%SST(*LDA 036 001)) *NE '0') GOTO FIN
             CLRPFM     VALCHED
             CALL       VALCHED
             CALL       PGM(RTVFRN) PARM(VALCHED &RCDNBR)

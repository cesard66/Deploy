             SNDPGMMSG  MSG('LA TRASMISION DEBE HACERSE DESDE CASA +
                          MATRIZ:') TOPGMQ(*EXT) MSGTYPE(*COMP)
             PAUSE      MSG('USUARIO: ' *CAT &USER *TCAT ' PERTENESE +
